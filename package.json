{
  "name": "node-typescript-graphql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prettier": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "lint:fix": "npx eslint --fix '**/*.ts'",
    "lint:find": "npx eslint '**/*.ts",
    "transpile:dev": "tsc --watch",
    "transpile:prod": "tsc",
    "start:dev": "npx pm2 startOrGracefulReload ecosystem.config.js --env development",
    "start:stage": "pm2 startOrGracefulReload ecosystem.config.js --env staging",
    "start:prod": "pm2-runtime ecosystem.config.js --env production",
    "stop": "npx pm2 stop ecosystem.config.js && npx pm2 delete ecosystem.config.js",
    "restart": "npx pm2 restart ecosystem.config.js",
    "list": "pm2 list",
    "develop": "yarn transpile:dev && yarn start:dev",
    "stage": "yarn transpile:prod && yarn start:stage",
    "start": "yarn transpile:prod && yarn start:prod"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.6.0",
    "apollo-server-express": "^2.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.10.0",
    "graphql": "^15.5.0",
    "mongoose": "5.10.18",
    "type-graphql": "^1.1.1",
    "winston": "^3.3.3"
  }
}
